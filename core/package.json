{
  "name": "@structura/core",
  "version": "0.0.1",
  "private": true,
  "description": "Shared types and utilities for Structura.",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.js"
    },
    "./imports/*": {
      "types": "./dist/imports/*.d.ts",
      "require": "./dist/imports/*.js",
      "import": "./dist/imports/*.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json"
  },
  "engines": {
    "node": ">=20.19.0"
  },
  "dependencies": {
    "tree-sitter": "^0.25.0"
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "typescript": "^5.9.3"
  }
}
